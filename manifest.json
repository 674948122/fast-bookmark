{
  "manifest_version": 3,
  "name": "Fast Bookmark",
  "version": "1.1.1",
  "description": "Fast Bookmark is a modern Chrome extension that evokes a sidebar suspended above the web page through shortcut keys or clicking on the icon, allowing you to quickly browse, search and manage your bookmarks without displaying the browser's native bookmarks bar. It is beautifully designed, supports dark mode, and does not squeeze the visual space of your web page at all.",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "bookmarks",
    "favicon",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "_favicon/*"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": []
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/fuse.basic.min.js",
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "action": {
    "default_title": "__MSG_extensionName__",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}